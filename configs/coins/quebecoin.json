{
  "coin": {
    "name": "Quebecoin",
    "shortcut": "QBC",
    "label": "Quebecoin",
    "alias": "quebecoin"
  },
  "ports": {
    "backend_rpc": 10889,
    "backend_message_queue": 38344,
    "blockbook_internal": 20889,
    "blockbook_public": 11889
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpcp",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-quebecoin",
    "package_revision": "satoshilabs-1",
    "system_user": "quebecoin",
    "version": "0.16.4.0",
    "binary_url": "https://github.com/QuebecoinQBC/quebecoin/releases/download/latest-wallets/quebecoin-ub18.04.tar.gz",
    "verification_type": "sha256",
    "verification_source": "476ffd59ea039762c4be1b3ea84486acd9574741cf7ae10279bbf09926233ced",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "bin/quebecoin-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/dogecoind -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "discover": 0,
      "rpcthreads": 1,
      "upnp": 0,
      "whitelist": "127.0.0.1"
    }
  },
  "blockbook": {
    "package_name": "blockbook-quebecoin",
    "system_user": "blockbook-quebecoin",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "-resyncindexperiod=30011 -resyncmempoolperiod=2011",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 76067358,
      "slip44": 90,
      "additional_params": {
        "fiat_rates": "coingecko",
        "fiat_rates_params": "{\"url\": \"https://api.coingecko.com/api/v3\", \"coin\": \"quebecoin\", \"periodSeconds\": 60}"
      }
    }
  },
  "meta": {
    "package_maintainer": "CryptoLover705",
    "package_maintainer_email": "support@cryptoloverbots.com"
  }
}
